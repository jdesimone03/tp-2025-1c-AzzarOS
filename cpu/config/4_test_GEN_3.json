{
    "ip_cpu": "${IP_CPU}",
    "port_cpu": "${PORT_CPU}",

    "ip_memory": "${IP_MEMORY}",
    "port_memory": "${PORT_MEMORY}",

    "ip_kernel": "${IP_KERNEL}",
    "port_kernel": "${PORT_KERNEL}",

    "tlb_entries": 256,
    "tlb_replacement": "FIFO",
